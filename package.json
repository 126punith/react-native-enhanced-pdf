    },
    "keywords": [
        "react-component",
        "react-native",
        "android",
        "ios",
        "pdf",
        "view",
        "viewer",
        "jsi",
        "performance",
        "enhanced",
        "native",
        "bridge",
        "high-performance"
    ],
    "author": {
        "name": "Punith M",
        "email": "punithm300@gmail.com",
        "url": "https://github.com/126punith"
    },
    "license": "MIT",
        "homepage": "https://github.com/126punith/react-native-enhanced-pdf",
    "bugs": {
        "url": "https://github.com/126punith/react-native-enhanced-pdf/issues"
    },
    "dependencies": {
        "crypto-js": "4.2.0",
        "deprecated-react-native-prop-types": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/runtime": "^7.20.1",
        "prop-types": "^15.7.2"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*",
        "react-native-blob-util": ">=0.13.7"
    },
    "files": [
        "android/",
        "ios/",
        "windows/",
        "src/",
        "DoubleTapView.js",
        "index.d.ts",
        "index.js",
        "index.js.flow",
        "PdfManager.js",
        "PdfPageView.js",
        "PdfView.js",
        "PdfViewFlatList.js",
        "PinchZoomView.js",
        "react-native-pdf.podspec",
        "fabric/",
        "README_JSI.md",
        "INTEGRATION_GUIDE.md"
    ],
    "codegenConfig": {
        "name": "rnpdf",
        "type": "components",
        "jsSrcsDir": "./fabric",
        "android": {
          "javaPackageName": "org.wonday.pdf"
        }
